<link rel="stylesheet" href="/css/bootstrap.min.css">
<%- include('head') %>
  <style>
    .table-box{
      width: 900px;
      float: left;
      height: 600px;
      margin-top: 50px;
      margin-left: 100px;
    }
    *{
      margin: 0 auto;
      padding: 0;
    }
    table{
      width: 900px;
      text-align: center;
      border-collapse: collapse;
    }
    table tr th,tr td{
      color:#000000;
      height: 30px;
      line-height: 30px;
      border:1px solid #d9e0ee;
    }
  </style>
  <div class="table-box">
    <table>
      <thead>
      <tr>
        <th>id</th>
        <th>商店名</th>
        <th>商店商品数量</th>
        <th>商店是否自营</th>
        <th>商店是否包邮</th>
        <th>商店是否满减</th>
        <th>商店是否满赠</th>
        <th>编辑</th>
        <th>删除</th>
      </tr>
      </thead>
      <tbody>
      <% for(var i=0;i<info.length;i++){ %>
      <tr>
        <td><%= info[i].store_id %></td>
        <td><%= info[i].store_name %></td>
        <td><%= info[i].store_spnum %></td>
        <td><%= info[i].store_iszy %></td>
        <td><%= info[i].store_isby %></td>
        <td><%= info[i].store_ismj %></td>
        <td><%= info[i].store_ismz %></td>
        <td><a href="/users/updatestore/<%= info[i].store_id %>">编辑</a></td>
        <td><a href="/users/deletestore/<%= info[i].store_id %>">删除</a></td>
      </tr>
      <% } %>
      </tbody>
    </table>
  </div>

